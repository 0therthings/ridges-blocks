{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sojourn/event-preview",
	"version": "0.1.0",
	"title": "Event Preview",
	"category": "sojourn",
	"icon": "calendar-alt",
	"description": "The event preview block allows you to showcase an upcoming event with a photo, date, and description",
	"supports": {
		"html": false
	},
	"textdomain": "event-preview",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
        "imgURL": {
            "type": "string",
            "source": "attribute",
            "attribute": "src",
            "selector": "img"
        },
        "imgID": {
            "type": "number"
        },
        "imgAlt": {
            "type": "string",
            "source": "attribute",
            "attribute": "alt",
            "selector": "img"
        }
	}
}
